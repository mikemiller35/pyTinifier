{
    "containerDefinitions": [
        {
            "environment": [
                {
                    "name": "POSTGRES_DB",
                    "value": "pytinifier"
                },
                {
                    "name": "POSTGRES_USER",
                    "value": "pytinifier"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "password"
                }
            ],
            "essential": true,
            "image": "postgres:12.1",
            "name": "db",
            "memory": "515 mb",
            "portMappings": [
                {
                    "containerPort": 5432,
                    "hostPort": 5432
                }
            ]
        },
        {
            "environment": [
                {
                    "name": "DBHOST",
                    "value": "db"
                },
                {
                    "name": "DBUSER",
                    "value": "pytinifier"
                },
                {
                    "name": "DBPASS",
                    "value": "password"
                },
                {
                    "name": "DBDB",
                    "value": "pytinifier"
                },
                {
                    "name": "FULL_URL",
                    "value": "http://localhost:5000/"
                }
            ],
            "essential": true,
            "image": "mmillerlb/pytinifier:latest",
            "name": "pytinifier",
            "memory": "515 mb",
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000
                }
            ]
        }
    ],
    "family": "",
    "volumes": []
}
